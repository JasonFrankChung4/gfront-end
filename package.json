{
  "name": "caribou",
  "version": "0.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "analyze": "webpack --config ./config/webpack/webpack.analyze.js",
    "build": "webpack --config ./config/webpack/webpack.prod.js",
    "deploy-to-gh-pages": "ASSET_PATH='/caribou/' OUTPUT_RELATIVE_PATH='/dist/gh-pages' npm run build && CODE_GOV_RELATIVE_DIR='/dist/gh-pages' CODE_GOV_BRANCH='gh-pages' npm run publish",
    "deploy-to-staging": "ASSET_PATH='/' OUTPUT_RELATIVE_PATH='/dist/federalist-stag' npm run build && CODE_GOV_RELATIVE_DIR='/dist/federalist-stag' CODE_GOV_BRANCH='federalist-stag' CODE_GOV_REPO='git@github.com:GSA/code-gov-web' npm run publish",
    "deploy-to-production": "ASSET_PATH='/' OUTPUT_RELATIVE_PATH='/dist/federalist-prod' npm run build && CODE_GOV_RELATIVE_DIR='/dist/federalist-prod' CODE_GOV_BRANCH='federalist-prod' npm run publish",
    "download-data": "wget https://raw.githubusercontent.com/GSA/code-gov-data/master/schemas/schema-2.0.0.json -O ./assets/data/schema.json",
    "install-about-plugin": "cp ./node_modules/@code.gov/about-page/index.js ./src/components/plugins/about-page.js && cp -fr ./node_modules/@code.gov/about-page/pages/html/* ./assets/plugins/about-page/.",
    "install-fscp-plugin": "cp ./node_modules/@code.gov/fscp-react-component/src/index.js ./src/components/plugins/policy-guide.js && cp ./node_modules/@code.gov/fscp-react-component/pages/html/* ./assets/plugins/fscp-react-component/.",
    "publish": "node publish.js",
    "start": "node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js --config ./config/webpack/webpack.dev.js",
    "start-on-server": "node --require dotenv/config ./node_modules/webpack-dev-server/bin/webpack-dev-server.js --config ./config/webpack/webpack.dev.js --host 0.0.0.0 --port 80",
    "webpack": "node ./node_modules/webpack/bin/webpack.js",
    "update-fonts": "cp node_modules/@code.gov/code-gov-font/font/* ./assets/fonts/."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GSA/caribou.git"
  },
  "author": "Daniel J. Dufour",
  "license": "CC0-1.0",
  "bugs": {
    "url": "https://github.com/GSA/caribou/issues"
  },
  "homepage": "https://github.com/GSA/caribou#readme",
  "private": "true",
  "dependencies": {
    "@code.gov/api-client": "0.2.7",
    "@code.gov/cautious": "0.3.0",
    "@code.gov/code-gov-font": "^0.7.0",
    "@code.gov/code-gov-style": "1.10.7",
    "@code.gov/compliance-dashboard-web-component": "^0.1.1",
    "@code.gov/fscp-react-component": "0.2.0",
    "@code.gov/json-schema-validator-web-component": "^0.1.0",
    "@code.gov/json-schema-web-component": "^0.1.0",
    "ajv": "^6.5.5",
    "custom-event-polyfill": "^1.0.6",
    "devicons": "^1.8.0",
    "dotenv": "^6.1.0",
    "history": "^4.7.2",
    "lodash.assign": "^4.2.0",
    "lodash.get": "^4.4.2",
    "lodash.intersection": "^4.4.0",
    "lodash.set": "^4.3.2",
    "offline-plugin": "^5.0.5",
    "prop-types": "^15.6.2",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-loadable": "^5.5.0",
    "react-redux": "^5.1.0",
    "react-router": "^4.4.0-beta.4",
    "react-router-dom": "^4.3.1",
    "recompose": "^0.30.0",
    "redux": "^4.0.1",
    "url-search-params-polyfill": "^5.0.0",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.1.2",
    "webpack-create-file-plugin": "^1.0.0",
    "webpack-dev-server": "^3.1.10",
    "webpack-merge": "^4.1.4",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-arrow-functions": "^7.0.0",
    "@babel/plugin-transform-classes": "^7.1.0",
    "@babel/plugin-transform-regenerator": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-dynamic-import-node": "^2.2.0",
    "babel-plugin-transform-function-bind": "^6.22.0",
    "clean-webpack-plugin": "^0.1.19",
    "cname-webpack-plugin": "^1.0.3",
    "connected-react-router": "^4.5.0",
    "copy-webpack-plugin": "^4.5.4",
    "css-loader": "^1.0.1",
    "favicons-webpack-plugin": "0.0.9",
    "file-loader": "^2.0.0",
    "gh-pages": "^2.0.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "markdown-loader": "^4.0.0",
    "mini-css-extract-plugin": "^0.4.4",
    "redux-thunk": "^2.3.0",
    "sass": "^1.14.1",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.23.1"
  }
}
